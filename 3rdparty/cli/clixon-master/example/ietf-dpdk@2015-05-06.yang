module ietf-dpdk {
    namespace "urn:ietf:params:xml:ns:yang:ietf-dpdk";

    prefix "dp";

    import ietf-yang-types {
	prefix "yang";
	revision-date "2013-07-15";
    }

    leaf leaf-testing {
	type string;
	description "Leaf only testing";
    }

    container system {
	leaf host-name {
	    type string;
	    description "Hostname for this system";
	}

	leaf-list domain-search {
	    type string;
	    description "List of domain names to search";
	}

	container login {
	    leaf message {
		type string;
		description
		    "Message given at start of login session";
	    }

	    list user {
		key "name";
		leaf name {
		    type string;
		}
		leaf full-name {
		    type string;
		}
		leaf class {
		    type string;
		}
	    }
	}
    }
}

